{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build downloader",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/I",
                "./thirdparty\\include",
                "/Fe:",
                "${workspaceFolder}\\bin\\downloader.exe",
                "src\\main.cpp",
                "cryptlib.lib",
                "/link",
                "/libpath:${workspaceFolder}\\thirdparty\\lib",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "detail": "调试器生成的任务。"
        },
